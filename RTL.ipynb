    "#RTL\n",
    "from pymavlink import mavutil\n",
    "\n",
    "connection = mavutil.mavlink_connection('/dev/ttyUSB0', baud=57600)\n",
    "connection.wait_heartbeat()\n",
    "\n",
    "connection.set_mode_apm(\"RTL\")\n",
    "print(\"Return to Launch mode set\")\n"
   ]
